if #ProfilesTextList["ObjectList"] == 0 then
	table.insert(ProfilesTextList["ObjectList"], "default")
	ProfilesTextList["RefreshValues"](ProfilesTextList["ObjectList"])
end
GUIbind["Reload"]()
GuiLibrary["UpdateUI"]()
